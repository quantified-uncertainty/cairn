{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@data": ["./src/data/index.ts"],
      "@data/*": ["./src/data/*"],
      "@lib/*": ["./src/lib/*"]
    }
  }
}
